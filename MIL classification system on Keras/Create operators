POSITIVE_CLASS = 2
BAG_COUNT = 3802
VAL_BAG_COUNT = 540
BAG_SIZE = 2
PLOT_SIZE = 4
ENSEMBLE_AVG_COUNT = 2

# POSITIVE_CLASS: Кол-во классов в полож мешке
# BAG_COUNT: Кол-во тренировачных мешков
# VAL_BAG_COUNT: Кол-во валидационных мешков
# BAG_SIZE: Кол-во экземпляров в мешке
# PLOT_SIZE: Кол-во мешков для plot
# ENSEMBLE_AVG_COUNT: Количество моделей для создания и усреднение вместе. 
    #(Необязательно: часто приводит к лучшей производительности - установлено значение 1 для одной модели)
